SELECT latlong, ownername || '<br>' || owneraddress
FROM ownertraploan
INNER JOIN owner ON ownertraploan.OwnerID = owner.ID
WHERE latlong is not null AND latlong <> '' AND (ReturnDate Is Null OR ReturnDate > '$CURRENT_DATE$')
ORDER BY ownername
