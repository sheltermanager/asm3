AND o.IsDeceased = 0 
AND (o.MembershipExpiryDate > '$CURRENT_DATE$' OR o.MembershipExpiryDate Is NULL)
AND GDPRContactOptIn LIKE '%post%' AND GDPRContactOptIn NOT LIKE '%email%'
