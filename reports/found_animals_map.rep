###
SELECT latlong, ownername || '<br>' || owneraddress || '<br>' || emailaddress || ' ' || mobiletelephone
FROM animalfound
INNER JOIN owner ON animalfound.OwnerID = owner.ID
WHERE returntoownerdate is null AND latlong is not null AND latlong <> ''
ORDER BY ownername
