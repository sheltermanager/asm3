<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sheltermanager.asm" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ASM</name>
    <description>
        ASM mobile application
    </description>
    <author email="support@sheltermanager.com" href="http://sheltermanager.com">
        sheltermanager.com team
    </author>
    <content src="index.html" />
    <icon src="www/img/logo.png" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
